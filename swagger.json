          - "private_key_jwt"
          - "none"
          description: "Authentication method for token endpoint, oidc specific"
        token_endpoint_auth_signing_alg:
          type: "string"
          enum:
            - "HS256"
            - "HS384"
            - "HS512"
            - "RS256"
            - "RS384"
            - "RS512"
            - "PS256"
          description: "Signing algorithm for the token endpoint, oidc specific"
        jwks:
          type: "object"
          properties:
            keys:
              type: "array"
              items:
                type: "string"
          description: "Public key value for the token endpoint, oidc specific"
        jwks_uri:
          type: "string"
          description: "Uri containing public key for the token endpoint, oidc specific"
        country:
          type: "string"
          description: "Country of the organisation of the service"
        website_url:
          type: "string"
